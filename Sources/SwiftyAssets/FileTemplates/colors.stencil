        {% filter removeExtraNewLines %}
        {% for color in colors %}
        static var {{ color.name }}: Color {
            return Color("{{ color.name }}")
        }
        {% ifnot forloop.last %}
        {% removeExtraNewLinesForceNewLine %}
        {% endif %}
        {% endfor %}
        {% endfilter %}
    }
}
