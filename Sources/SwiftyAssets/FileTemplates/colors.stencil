#if canImport(UIKit)
import UIKit

// swiftlint:disable force_unwrapping
extension SwiftyAssets {
    class UIColors {
        {% filter removeExtraNewLines %}
        {% for color in colors %}
        static var {{ color.name }}: UIColor {
            return UIColor(named: "{{ color.name }}")!
        }
        {% ifnot forloop.last %}
        {% removeExtraNewLinesForceNewLine %}
        {% endif %}
        {% endfor %}
        {% endfilter %}
    }
}
